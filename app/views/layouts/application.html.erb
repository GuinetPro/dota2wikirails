<!DOCTYPE html>
<html>
<head>
  <title>Dota</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>

</head>
<body>

<header class="navbar navbar-static-top bs-docs-nav" id="top" role="banner">
  <div class="container">
    <div class="navbar-header">
      <button class="navbar-toggle collapsed" type="button" data-toggle="collapse" data-target="#bs-navbar" aria-controls="bs-navbar" aria-expanded="false">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>

      <%=link_to( image_tag("Wiki.png"), root_path , :class=>"navbar-brand") %>

    </div>
    <nav id="bs-navbar" class="collapse navbar-collapse navbar-default">
      <ul class="nav navbar-nav">
        <li>
          <a href="../getting-started/">Items</a>
        </li>
        <li>
          <%= link_to "Store", heroe_sets_path %>
        </li>
      </ul>
      <ul class="nav navbar-nav navbar-right">

        <% if user_signed_in? %>
          <li>
          <%= link_to "Log out", destroy_user_session_path, method: :delete %>
          </li>
        <% else %>

          <li>
            <%= link_to "Sign in ", new_user_session_path %>
          </li>

          <li>
          <%= link_to "Sign out ", new_user_registration_path %>
          </li>
        <% end %>

      </ul>
    </nav>
  </div>
</header>

    <div class="container">
        <%= yield %>
    </div>


</body>
</html>
